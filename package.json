{
  "name": "webpack-node",
  "version": "1.0.0",
  "description": "Node.js w/ webpack. :hammer:",
  "main": "server.js",
  "scripts": {
    "start": "npm run prod && node ./dist/server.js",
    "dev": "./node_modules/.bin/webpack --mode development --watch",
    "prod": "./node_modules/.bin/webpack --mode production",
    "init": "npm i -S body-parser cors express mongoose && npm -D i @babel/core @babel/preset-env babel-core babel-loader nodemon-webpack-plugin webpack webpack-cli webpack-node-externals"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuksamardzic/webpack-node.git"
  },
  "author": "Vuk Samardžić <samardzic.vuk@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/vuksamardzic/webpack-node/issues"
  },
  "homepage": "https://github.com/vuksamardzic/webpack-node#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^5.7.7"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "nodemon-webpack-plugin": "^4.1.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  }
}
